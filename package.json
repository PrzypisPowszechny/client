  "scripts": {
    "help": "npm-scripts-help",
    "build-client": "webpack --mode development --config config/client/prod.config.js",
    "start-client": "webpack-dev-server --mode development --config config/client/dev.config.js",
    "start": "node loadExtensionToChromiumServer.js & webpack --mode development --config config/browser-extension/ext.dev.config.js --watch",
    "build": "webpack --mode development --config config/browser-extension/ext.dev.config.js",
    "build-optimized": "webpack --mode production --config config/browser-extension/ext.prod.config.js",
    "build-package": "npm run build-optimized && npm run pack",
    "release": "rm -rf node_modules && npm install && PP_API=pp PP_DEV=false npm run build-optimized && npm run pack",
    "pack": "cd ./dist && zip -r pp-chrome.zip ./browser-extension",
    "lint": "tslint --project tsconfig.json",
    "lint-fix": "npm run lint -- --fix",
    "test-unit": "jest src",
    "test-unit-full": "npm run build && npm run test-unit",
    "test-e2e": "jest e2e --maxWorkers=1",
    "test-e2e-full": "npm run build && npm run test-e2e",
    "test": "jest --maxWorkers=1",
    "validate-annotations": "PP_DEV_SENTRY_ALT=true PP_DEV_SENTRY_UNLOCATED_IGNORE=true npm run build && PP_API=devdeploy1 jest validate-annotations"
  },
